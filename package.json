{
  "name": "search-tinymt-seed",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/mizdra/search-tinymt-seed.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:wasm": "cargo build --release --target wasm32-unknown-unknown",
    "postbuild:wasm": "wasm-bindgen target/wasm32-unknown-unknown/release/lib.wasm --out-dir src/wasm",
    "build:js": "webpack --mode production",
    "build": "run-s build:wasm build:js",
    "dev:wasm": "cargo watch -i 'src/wasm/{lib_bg.wasm,lib.d.ts,lib.js}' -s 'yarn run build:wasm'",
    "dev:js": "webpack-dev-server --mode development",
    "dev": "run-p dev:wasm dev:js",
    "lint": "eslint --ignore-pattern 'src/wasm' 'src/**/*.{js,ts,vue}'",
    "test": "cargo test --release -- --ignored --test-threads=2"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "@vue/eslint-config-typescript": "^3.0.0-beta.6",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-vue": "^4.3.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.0.6",
    "npm-run-all": "^4.1.2",
    "ts-loader": "^5.1.0",
    "typescript": "^3.0.3",
    "vue": "^2.5.16",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@mizdra/tinymt": "^1.0.0",
    "element-ui": "^2.2.2",
    "tslib": "^1.9.0"
  }
}
